{
  "name": "simply-doc",
  "version": "1.0.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "npm --no-git-tag-version version patch",
    "build-production": "npm run prebuild && ng build --configuration=production --optimization --aot --build-optimizer --output-hashing=all",
    "build-qa": "npm run prebuild && ng build --configuration=testing --optimization --aot --build-optimizer --output-hashing=all",
    "build-qa2": "npm run prebuild && ng build --configuration=testing2 --optimization --aot --build-optimizer --output-hashing=all",
    "build-dev": "npm run prebuild && ng build --configuration=devhost --optimization --aot --build-optimizer --output-hashing=all",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch --no-progress --browsers=ChromeHeadlessCI"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.2.3",
    "@angular/cdk": "^13.2.3",
    "@angular/common": "~13.2.3",
    "@angular/compiler": "~13.2.3",
    "@angular/core": "~13.2.3",
    "@angular/forms": "~13.2.3",
    "@angular/platform-browser": "~13.2.3",
    "@angular/platform-browser-dynamic": "~13.2.3",
    "@angular/router": "~13.2.3",
    "@ckeditor/ckeditor5-angular": "^2.0.2",
    "@ckeditor/ckeditor5-build-classic": "^32.0.0",
    "@fullcalendar/angular": "^5.10.2",
    "@fullcalendar/daygrid": "^5.10.1",
    "@fullcalendar/list": "^5.10.1",
    "@fullcalendar/timegrid": "^5.10.1",
    "moment": "^2.29.2",
    "ng-zorro-antd": "^13.0.1",
    "rxjs": "~7.5.4",
    "tippy.js": "^6.3.7",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1302.4",
    "@angular-devkit/build-angular": "~13.2.4",
    "@angular/cli": "~13.2.4",
    "@angular/compiler-cli": "~13.2.3",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^17.0.18",
    "autoprefixer": "^10.4.2",
    "jasmine-core": "~4.0.0",
    "jsonc-parser": "^3.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "postcss": "^8.4.6",
    "tailwindcss": "^3.0.23",
    "typescript": "~4.5.5"
  }
}
